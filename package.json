{
  "name": "react-form-with-constraints",
  "version": "0.1.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/tkrotoff/ReactFormWithConstraints.git"
  },
  "main": "lib/FormWithConstraints.js",
  "types": "lib/FormWithConstraints.d.ts",
  "license": "MIT",

  "scripts": {
    "clean": "del-cli build lib",

    "build": "webpack -d --env.development",
    "build:watch": "webpack -d --env.development --watch",

    "prepare": "tsc",

    "pretest": "npm run clean && tsc || true",
    "test": "node --trace-warnings node_modules/.bin/jest --no-cache",
    "pretest:silent": "npm run pretest",
    "test:silent": "node --trace-warnings node_modules/.bin/jest --no-cache --silent"
  },

  "dependencies": {
    "react": "latest",
    "prop-types": "latest",
    "react-dom": "latest"
  },

  "devDependencies": {
    "typescript": "latest",
    "tslint": "latest",

    "@types/react": "latest",
    "@types/prop-types": "latest",
    "@types/react-dom": "latest",

    "ts-node": "latest",
    "@types/node": "latest",

    "babel-core": "latest",
    "babel-preset-react": "latest",

    "webpack": "latest",
    "@types/webpack": "latest",
    "ts-loader": "latest",
    "babel-loader": "latest",
    "file-loader": "latest",

    "del-cli": "latest",

    "jest": "latest",
    "@types/jest": "latest",

    "enzyme": "latest",
    "@types/enzyme": "latest",
    "react-test-renderer": "latest"
  },

  "engines": {
    "node": "^6"
  }
}
