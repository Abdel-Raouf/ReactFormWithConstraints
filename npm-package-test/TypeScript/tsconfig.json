{
  "compilerOptions": {
    "target": "es2017",
    "jsx": "react",
    "module": "commonjs",
    "moduleResolution": "node",
    "noLib": false,
    "lib": ["dom.iterable", "es2017"],
    "sourceMap": true,
    "declaration": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true
  }
}
